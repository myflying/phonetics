@import "../price_dialog/price_dialog.wxss";

page {
  background: #f8f8f8;
  width: 100%;
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
}

.user-info {
  width: 750rpx;
  height: 300rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  background: url('data:image/png;base64,');
  background-size: 100% 100%;
}

.user-head {
  width: 122rpx;
  height: 122rpx;
  border-radius: 50%;
  border:2px solid #fff;
}

.user-login {
  height: 100rpx;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}

.user-name {
  height: 66rpx;
  line-height: 66rpx;
  color: #fff;
  font-size: 14pt;
  margin-top: 20rpx;
  font-weight: 600;
}

.login-btn-view {
  margin-top: 20rpx;
  height: 100rpx;
}

.login-btn {
  width: 200rpx;
  height: 66rpx;
  line-height: 66rpx;
  background: #fff;
  color: #000;
  font-size: 11pt;
}


.login-vip-btn {
  width: 670rpx;
  height: 100rpx;
  position: absolute;
  background: rgba(255, 255, 255, 0);
  left: 0;
  top: 0;
}

.button,button::after{
  border: 0;
}

.order-top {
  width: 670rpx;
  height: 200rpx;
  background: #fff;
  border-radius: 10px;
  margin-top: 40rpx;
}

.vip-wrapper {
  width: 670rpx;
  height: 100rpx;
  position: relative;
}

.vip {
  width: 670rpx;
  height: 100rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-between;
  border-bottom: 1px solid #eeeeee;
}

.vip-left {
  width: 240rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  flex-direction: row;
}

.open-vip-img {
  width: 52rpx;
  height: 52rpx;
}

.open-vip-txt {
  color: #000;
  font-size: 12pt;
  height: 52rpx;
  line-height: 52rpx;
  margin-left: 20rpx;
}

.more {
  width: 50rpx;
  height: 50rpx;
  margin-right: 20rpx;
}

.order {
  width: 670rpx;
  height: 100rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-between;
}

.order-left { 
  width: 240rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  flex-direction: row;
}

.order-img {
  width: 52rpx;
  height: 52rpx;
}

.order-txt {
  color: #000;
  font-size: 12pt;
  height: 52rpx;
  line-height: 52rpx;
  margin-left: 20rpx;
}


.order-bottom {
  width: 670rpx;
  height: 100rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-between;
  background: #fff;
  border-radius: 10px;
  margin-top: 20rpx;
}

.vowel-left {
  width: 240rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  flex-direction: row;
  margin-left: 16rpx;
}

.vowel-error-img {
  width: 52rpx;
  height: 52rpx;
}

.vowel-error-txt {
  color: #000;
  font-size: 12pt;
  height: 52rpx;
  line-height: 52rpx;
  margin-left: 20rpx;
}
